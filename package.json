{
  "name": "semantic-release-demo",
  "version": "1.2.12",
  "description": "A test project for using semantic-release",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "build:prod": "tsc --build tsconfig.prod.json",
    "debug": "ts-node-dev --respawn ./src",
    "semantic-release": "semantic-release --no-ci --dry-run false",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karsei/semantic-release-demo.git"
  },
  "author": "Karsei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Karsei/semantic-release-demo/issues"
  },
  "homepage": "https://github.com/Karsei/semantic-release-demo#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "semantic-release": "^21.1.2"
  }
}
